    <section class="section-background">
        
        <h1 class="text-center">Fleet</h1>


        <div class="container">
            <div class="row">
                @foreach (var item in Model)
                {

                    <div class="col-md-4 col-sm-5">
                        <div class="courses-thumb courses-thumb-secondary">
                            <div class="courses-top">
                                <div class="courses-image">
                                    <img src="~/assets1/images/product-2-720x480.jpg" class="img-responsive" alt="">
                                </div>
                            </div>

                            <div class="courses-detail">
                                <h3>
                                    <a href="fleet.html">
                                        @Html.DisplayFor(modelItem => item.VehicleName)
                                    </a>
                                </h3>
                                <p>
                                    @Html.DisplayFor(modelItem => item.VehicleTypeName)
                                </p>
                                <a href="@Url.Action("FleetID", new { id = item.VehicleId })" class="text-primary">
                                    View more...
                                </a>
                            </div>
                         @*    <div class="courses-info">
                                <button type="button" data-toggle="modal" data-target=".bs-example-modal" class="section-btn btn btn-primary btn-block">Book Now</button>
                            </div> *@
                        </div>
                    </div>

                }
                  </div>
        </div>
    </section>


